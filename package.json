    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "react-toastify": "^8.1.1",
    "styled-components": "^5.3.3",
    "web-vitals": "^2.1.4"
